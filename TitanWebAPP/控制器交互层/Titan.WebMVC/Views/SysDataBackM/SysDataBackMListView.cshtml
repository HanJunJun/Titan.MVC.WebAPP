@model Titan.Controllers.ViewModel.SysDataBackMViewList
@{
    Layout = null;
}

<!-- 模块标题 -->
<div class="block-header">
    <h2>数据库管理</h2>
</div>
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <ul class="header-dropdown m-r--5">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            <i class="material-icons">more_vert</i>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="javascript:void(0);">帮助</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div class="body">
                <span>操作时间：</span>
                <input id="startTime" name="startTime" type="text" class="laydate-icon date-commonlist" readonly="readonly" value="" onfocus="this.blur();" />
                <span>-</span>
                <input id="endTime" name="endTime" type="text" class="laydate-icon date-commonlist" readonly="readonly" value="" onfocus="this.blur();" />
                <div id="DataTableView">
                    <!-- 分页数据 -->
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $("#Param1").keydown(function (e) {
        if (e.keyCode == 13) {
            pageSumit($("#Param1").val(), $('#Param2 option:selected').val());
        }
    });
    $("#update").click(function () {
        pageSumit($("#Param1").val(), $('#Param2 option:selected').val());
    });
    $(document).ready(function () {
        pageSumit("","","");
    });

    function pageSumit(startTime, endTime,key, pageIndex, pageSize) {
        $.ajax({
            type: "GET",
            url: "/SysDataBackM/MemberShipMOtherListViewBy",
            data: { startTime: startTime, endTime: endTime,key:key, pageIndex: pageIndex, pageSize: pageSize },
            success: function (mess) {
                $('#DataTableView').empty();
                $('#DataTableView').html(mess);
            }
        });
    }
</script>